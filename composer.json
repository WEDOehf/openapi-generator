{
  "name": "wedo/openapi-generator",
  "license": "MIT",
  "type": "library",
  "description": "Open api json generator from typed classes",
  "keywords": ["swagger", "openapi", "api", "documentation", "generator"],
  "authors": [
    {
      "name": "Dalibor Korpar"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wedo\\OpenApiGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wedo\\OpenApiGenerator\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2|^8.0",
    "nette/utils": "^2.4|^3.0",
    "nette/finder": "^2.4",
    "nette/reflection": "^2.4"
  },
  "require-dev": {
    "ninjify/qa": "^0.10",
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "phpstan/phpstan": "^0.12.6",
    "phpstan/phpstan-deprecation-rules":"^0.12",
    "phpstan/phpstan-nette":"^0.12",
    "phpstan/phpstan-strict-rules":"^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
