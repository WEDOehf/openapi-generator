{
  "name": "wedo/openapi-generator",
  "license": "MIT",
  "type": "library",
  "description": "Open api json generator from typed classes",
  "keywords": ["swagger", "openapi", "api", "documentation", "generator"],
  "authors": [
    {
      "name": "Dalibor Korpar"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wedo\\OpenApiGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wedo\\OpenApiGenerator\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "nette/utils": "^3.0|^4.0",
    "nette/finder": "^2.4|^3.0"
  },
  "require-dev": {
    "contributte/qa": "^0.3",
    "phpunit/phpunit": "^9.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules":"^1.0",
    "phpstan/phpstan-nette":"^1.0",
    "phpstan/phpstan-strict-rules":"^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
